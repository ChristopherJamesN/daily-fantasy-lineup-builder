<p>
  <strong>Name:</strong>
  <%= @player.name %>
</p>

<p>
  <strong>Position:</strong>
  <%= @player.position %>
</p>

<p>
  <strong>Projected Points:</strong>
  <%= @player.projectedPoints %>
</p>

<p>
  <strong>Actual Points:</strong>
  <%= @player.actualPoints %>
</p>

<p>
  <strong>Lineups:</strong>
  <% @player.lineups.each do |lineup| %>
    <% if lineup.user_id == current_user.id %>
      <%= lineup.name %>
    <% end %>
  <% end %>
</p>

<%= link_to 'Edit', edit_player_path(@player) %> |
<%= link_to 'Back', players_path %>
